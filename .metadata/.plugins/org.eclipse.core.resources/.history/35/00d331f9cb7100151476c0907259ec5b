
public class Computer {

	private String name;
	private int age;


	private int getAge() {
		return age;
	}

	private void setAge(int age) {
		if (age <= 0) {
			System.out.println("Age is not valid (less or equal to 0)");
			return;
		}

		this.age = age;
	}

	private String getName() {
		return name;
	}

	void setName(String name) {
		
		this.name = name;
	}


	public void go() {
		System.out.println("I go home " + "and my name is " + getName());
	}

	public String speak() {
		return "Hey there!";
	}

	public void eat() {
		if (height > 0) {
			System.out.println("DND! I have lunch. My height is " + height);
		}
	}

}
