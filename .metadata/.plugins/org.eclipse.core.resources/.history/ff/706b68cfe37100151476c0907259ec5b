
public class BMI {
	// public String name;
	// public int age;
	// public int height;
	// public int weight;
	public int bmi;

	Computer c = new Computer();

	public void main(String[] args)

	{
		Human h = new Human();
		bmi = h.getHeight() - h.getHeight();
		System.out.println(bmi);
	}

	public void checkBMI() {
		Computer c = new Computer();
		Human h = new Human();
		if (bmi > 120) {
			System.out.println("Hey, " + h.getName() + "! You have to go to gym. Your BMI is " + bmi);
		}
		if (h.bmi < 100) {
			System.out.println("Hey, " + h.getName() + "! My name is  " + c.getName() + "I'm " + c.getAge());
		} else {
			System.out.println("Hey, " + h.getName() + "! My name is  " + c.getName()
					+ "I'm busy now. But you can call me later!");
		}
	}
}
